{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit to bootstrap a React project.",
  "main": "app.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "lint": "eslint src/*",
    "build": "webpack --config webpack.config.prod.js",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "dev": {
      "command": "node .",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "mocha tests/**/*.spec.js --compilers js:babel-register",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suranartnc/react-starter-kit.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Suranart Niamcome",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suranartnc/react-starter-kit/issues"
  },
  "homepage": "https://github.com/suranartnc/react-starter-kit#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.6.0",
    "better-npm-run": "0.0.7",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-react-hmre": "^1.1.0",
    "chai": "^3.5.0",
    "eslint": "^2.2.0",
    "faker": "^3.0.1",
    "json-server": "^0.8.8",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
